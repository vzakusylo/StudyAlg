{
  "app": {
    "name": "discounts-service"
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "discounts-service",
    "address": "docker.for.mac.localhost",
    "port": "5010",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "discounts-service",
    "requestRetries": 3
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "customers",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "usavc",
    "env": "local",
    "interval": 5
  },
  "rabbitMq": {
    "namespace": "reason-code",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  }
}